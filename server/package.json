{
    "name": "develop-denver",
    "version": "0.3.0",
    "description": "Develop Denver app",
    "main": "index.js",
    "private": true,
    "scripts": {
        "dev": "tsc-watch --onSuccess 'node dist/index.js'",
        "postinstall": "tsc",
        "dev:database:setup": "knex migrate:latest && knex seed:run",
        "test": "NODE_ENV=test mocha -r ts-node/register --reporter min tests/routes",
        "test:unit": "NODE_ENV=test mocha -wG --reporter min -r ts-node/register -r tests/test-setup.js tests/routes",
        "test:database:setup": "knex migrate:latest --env test",
        "start": "node dist/index.js"
    },
    "author": "Kyle Coberly",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.18.0",
        "bcrypt": "^3.0.0",
        "body-parser": "^1.18.3",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "crypto": "^1.0.1",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "helmet": "^3.12.1",
        "jsonwebtoken": "^8.3.0",
        "knex": "^0.15.0",
        "morgan": "^1.9.1",
        "nodemailer": "^4.6.8",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^7.4.3",
        "showdown": "^1.8.6",
        "stripe": "^6.7.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/express": "^4.16.1",
        "@types/node": "^11.13.0",
        "faker": "^4.1.0",
        "lodash": "^4.17.11",
        "mocha": "^5.2.0",
        "nodemon": "^1.17.5",
        "supertest": "^3.1.0",
        "ts-node": "^8.0.3",
        "tsc-watch": "^2.1.2",
        "typescript": "^3.4.1"
    },
    "engines": {
        "node": "^10.5.0"
    }
}
