{
    "name": "develop-denver",
    "version": "0.1.0",
    "description": "Develop Denver app",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon src/index.js",
        "dev:database:setup": "knex migrate:latest && knex seed:run",
        "test": "mocha --reporter min tests/routes",
        "test:unit": "mocha -wG --reporter min -r tests/test-setup.js tests/routes",
        "test:database:setup": "knex migrate:latest --env test",
        "start": "node src/index.js",
        "deploy": "git subtree push --prefix=server/ server master"
    },
    "author": "Kyle Coberly",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.3",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "helmet": "^3.12.1",
        "knex": "^0.15.0",
        "morgan": "^1.9.0",
        "pg": "^7.4.3"
    },
    "devDependencies": {
        "faker": "^4.1.0",
        "mocha": "^5.2.0",
        "nodemon": "^1.17.5",
        "supertest": "^3.1.0"
    }
}
