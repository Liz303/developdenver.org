{
  "name": "develop-denver",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:staging": "vue-cli-service build --mode staging",
    "build:production": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --reporter min -r tests/unit-setup.js",
    "test:unit:watch": "vue-cli-service test:unit --reporter min --watch -G -r tests/unit-setup.js",
    "test:e2e": "vue-cli-service test:e2e --headless"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/vue-fontawesome": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "change-case": "^3.0.2",
    "epic-spinners": "^1.0.3",
    "dayjs": "^1.7.5",
    "jwt-decode": "^2.2.0",
    "pluralize": "^7.0.0",
    "register-service-worker": "^1.0.0",
    "showdown": "^1.8.6",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persist": "^1.4.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.15",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-standard": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.16",
    "chai": "^4.1.2",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-editorconfig": "^0.2.1",
    "vue-template-compiler": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
